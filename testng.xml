<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TutorialNinga">
<listeners>
<listener class-name="listeners.Listener"></listener>
</listeners>

    <test  name="Registeration">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="testcases.TC01_Registeration">
                <methods>
                    <include name="Registeration_P"/>
                </methods>
            </class>
        </classes>
    </test>

    <test  name="Login with valid email and password">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="testcases.TC02_Login">
                <methods>
                    <include name="LoginWithValidEmailAndPassword_P"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>